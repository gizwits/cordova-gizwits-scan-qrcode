      "3.0.0": {
        "cordova": ">100"
      }
    }
  },
  "devDependencies": {
    "dmd-plugin-cordova-plugin": "^0.1.0",
    "husky": "^0.10.1",
    "jsdoc-to-markdown": "^1.2.0",
    "jshint": "^2.6.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gizwits/cordova-gizwits-scan-qrcode.git"
  },
  "bugs": {
