{
  "name": "gizscanqrcode",
  "version": "2.0.2-beta",
  "description": "兼容安卓13",
  "cordova": {
    "id": "com.gizscanqrcode",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "keywords": [
    "gizwits",
    "gizwits plugin",
    "gizscanqrcode",
    "giz scan qrcode",
    "qrcode",
    "scan qrcode",
    "scan code",
    "scan",
    "cordova gizwits",
    "cordova scan qrcode",
    "cordova qrcode",
    "cordova code",
    "qcode"
  ],
  "scripts": {
    "precommit": "npm run gen-docs && git add README.md",
    "gen-docs": "jsdoc2md --template \"jsdoc2md/TEMPLATE.md\" \"www/**/*.js\" --plugin \"dmd-plugin-cordova-plugin\" > README.md",
    "test": "npm run jshint",
    "jshint": "node node_modules/jshint/bin/jshint www && node node_modules/jshint/bin/jshint src && node node_modules/jshint/bin/jshint tests"
  },
  "author": "Gizwits Inc.",
  "license": "GPL-3.0",
  "engines": {
